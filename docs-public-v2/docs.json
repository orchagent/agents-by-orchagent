{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "orchagent",
  "logo": {
    "dark": "/logo/logo-icon-simple.png",
    "light": "/logo/logo-icon-simple.png"
  },
  "favicon": "/logo/logo-icon-simple.png",
  "colors": {
    "primary": "#f97316",
    "light": "#fb923c",
    "dark": "#ea580c"
  },
  "navbar": {
    "links": [
      {
        "label": "Dashboard",
        "href": "https://orchagent.io/dashboard"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Sign Up",
      "href": "https://orchagent.io"
    }
  },
  "navigation": {
    "languages": [
      {
        "language": "en",
        "tabs": [
          {
            "tab": "Documentation",
            "groups": [
              {
                "group": "Get Started",
                "pages": ["index", "quickstart", "troubleshooting"]
              },
              {
                "group": "Using Agents",
                "pages": [
                  "using-agents/api-reference",
                  "using-agents/cli-commands",
                  "using-agents/code-snippets"
                ]
              },
              {
                "group": "Building Agents",
                "pages": [
                  "building-agents/agent-types",
                  "building-agents/manifest-format",
                  "building-agents/environments",
                  "building-agents/publishing",
                  "building-agents/orchestration",
                  "building-agents/sdk"
                ]
              },
              {
                "group": "Concepts",
                "pages": [
                  "concepts/byok",
                  "concepts/local-vs-server",
                  "concepts/rate-limits",
                  "concepts/monetization"
                ]
              },
              {
                "group": "API Reference",
                "pages": ["api-reference/overview"]
              }
            ]
          }
        ]
      }
    ]
  },
  "footer": {
    "socials": {
      "github": "https://github.com/orchagent",
      "x": "https://twitter.com/orchagent"
    }
  },
  "contextual": {
    "options": [
      "copy",
      "view",
      "claude",
      "chatgpt",
      "cursor"
    ]
  },
  "openapi": ["/api-reference/openapi.json"],
  "api": {
    "baseUrl": "https://api.orchagent.io"
  },
  "redirects": [
    { "source": "/getting-started", "destination": "/quickstart" },
    { "source": "/docs/cli", "destination": "/using-agents/cli-commands" },
    { "source": "/docs/agents", "destination": "/building-agents/agent-types" },
    { "source": "/cli", "destination": "/using-agents/cli-commands" },
    { "source": "/agents", "destination": "/building-agents/agent-types" },
    { "source": "/skills", "destination": "/building-agents/orchestration" },
    { "source": "/sdk", "destination": "/building-agents/sdk" },
    { "source": "/api", "destination": "/api-reference/overview" },
    { "source": "/building-agents/skills-format", "destination": "/building-agents/orchestration" },
    { "source": "/docs/:path*", "destination": "/:path*" },
    { "source": "/pricing", "destination": "/concepts/monetization" },
    { "source": "/monetization", "destination": "/concepts/monetization" },
    { "source": "/billing", "destination": "/concepts/monetization" }
  ]
}
